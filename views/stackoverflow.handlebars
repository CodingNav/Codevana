<!--youtube-->
<div class="container-fluid">

  <h1>Stack Overflow</h1>
  <div class="row">
    {{#each posts as |post|}}
    <div class="col-lg-2">
      <a href="{{post.link}}">
        <div class="card w-100 h-100">
          <div class="card-body">
            <h5 class="card-title">{{post.title}}</h5>
          </div>
          <div class="card-footer">
            <h6 class="card-subtitle mb-2 text-muted">Upvotes: {{post.score}}</h6>
          </div>
        </div>
      </a>
    </div>
    {{/each}}
  </div>

</div>